.swiper-container{
  width: 100%;
  height: 266rpx;
  position: relative;
}
.swiper-container swiper{
  height: 100%;
  border-radius: 20rpx;
}
.swiper-container swiper image{
  width: 100%;
  height: 100%;
  
  transition: all 0.5s;
}
.dots{
  position: absolute;
  display: inline-flex;
}
.dots .dot{
  width: 8rpx;
  height: 8rpx;
  background-color: #fff;
  opacity: 0.5;
  border-radius: 4rpx;
  margin-left: 4rpx;
  
}
.dots .active{
  width: 29rpx;
  opacity: 1;
}